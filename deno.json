{
  "lock": false,
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@fartlabs/htx"
  },
  "tasks": {
    "dev": "deno run -A --watch main.ts example.tsx",
    "test": "deno test -A",
    "serve": "deno run -A serve.tsx"
  },
  "imports": {
    "@fartlabs/htx": "jsr:@fartlabs/htx@^0.0.9",
    "@std/assert": "jsr:@std/assert@^1.0.8",
    "@std/async": "jsr:@std/async@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.5",
    "@fartlabs/ht": "jsr:@fartlabs/ht@^0.0.9",
    "@std/http": "jsr:@std/http@^1.0.10",
    "ts-morph": "npm:ts-morph@^24.0.0"
  },
  "lint": {
    "exclude": ["example"]
  }
}
